<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

#http://ole.groups.curriki.org/
#http://www.curriki.org/groups/ole/

#Redirect 301 ^ole56.groups.curriki.org/$ http://www.yahoo.com  [R]

RewriteRule ^(.*)\.groups\.(.*)$ ./groups/$1 [R]

#Redirect 301 ^(.*).groups.curriki.org/(.*)$ http://www.curriki.org/groups/$1/  [R]
#RewriteRule ^groups.curriki.org www.curriki.org/goups
#RewriteRule ^(.*).groups.curriki.org/(.*)$ http://www.curriki.org/groups/$1/  [R]

RewriteRule ^xwiki/bin/view/(.*)$ /wp-content/themes/genesis-curriki/curriki-customized/xwiki/resource_redirect.php/?pageurl=$1 [QSA,L]
RewriteRule ^index\.php$ - [L]
RewriteRule ^oer/(.*)/(.*).xml$ https://currikicdn.s3-us-west-2.amazonaws.com/resourceswfs/$2.xml [QSA,L]
RewriteRule ^oer/(.*).xml$ https://currikicdn.s3-us-west-2.amazonaws.com/resourceswfs/$1.xml [QSA,L]
RewriteRule ^create-resource/(.*).xml$ https://currikicdn.s3-us-west-2.amazonaws.com/resourceswfs/$1.xml [QSA,L]
RewriteRule ^oer/(.*)$ /index.php/oer/?pageurl=$1&rid=%2 [QSA,L]
RewriteRule ^oer-widget/(.*)$ /index.php/oer/?pageurl=$1&rid=%2 [QSA,L]

RewriteRule ^members/(.*)/resources/? /curriki/user-library/?user=$1&memberpage=1 [QSA,L]

RewriteRule ^community/(.*)$ /index.php/community/?comm_url=$1&rid=%2 [PT,QSA,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
